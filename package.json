{
  "name": "react-video-timestone",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "description": "A React component library for video timestone functionality",
  "main": "./dist/react-video-timestone.umd.js",
  "module": "./dist/react-video-timestone.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-video-timestone.es.js",
      "require": "./dist/react-video-timestone.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "video",
    "timestone",
    "component",
    "library",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-icons": "^5.5.0",
    "vite-plugin-lib-inject-css": "^2.2.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-svgr": "^4.3.0"
  }
}
